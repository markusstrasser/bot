		"dotenv": "^16.4.5",
		"eslint-plugin-neverthrow": "^1.1.4",
		"find-up": "^7.0.0",
		"ignore": "^5.3.1",
		"memfs": "^4.9.3",
		"neverthrow": "^6.2.2",
		"vite": "^5.3.2",
		"vitest": "^1.6.0",
		"zod": "^3.23.8",
		"zod-to-json-schema": "^3.23.1"
	}
